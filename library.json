{
    "name": "no-OS-FatFS-SD-SPI-RPi-Pico.dev_sdio",
    "version": "1.0.0",
    "description": "Simple library for SD Cards on the RP2040",
    "keywords": ["Data Storage", "FatFs", "SD card", "Secure Digital card", "SPI", "SDIO"],
    "repository":
    {
      "type": "git",
      "url": "https://github.com/carlk3/no-OS-FatFS-SD-SPI-RPi-Pico.git#dev_sdio"
    },
    "authors":
    [
      {
        "name": "Carl Kugler",
        "email": "carlk3@gmail.com"
      }
    ],
    "license": "Apache-2.0 AND MIT-Modern-Variant",
    "frameworks": "*",
    "platforms": "wizio-pico",
    "build": {
      "srcFilter": [
          "-<.git/> -<.svn/>",
          "+<ff15/source/ff.c>",
          "+<ff15/source/ffsystem.c>",
          "+<ff15/source/ffunicode.c>",
          "+<rtc.c>",
          "+<f_util.c>",
          "+<glue.c>",
          "+<my_debug.c>",
          "+<rtc.c>",
          "+<sd_driver/SDIO/rp2040_sdio.c>",
          "+<sd_driver/SDIO/sd_card_sdio.c>",
          "+<sd_driver/sd_card.c>",
          "+<sd_driver/SPI/crc.c>",
          "+<sd_driver/SPI/sd_card_spi.c>",
          "+<sd_driver/SPI/sd_spi.c>",
          "+<sd_driver/SPI/spi.c>" 
      ],
      "flags": [
        "-D _GNU_SOURCE",
        "-D __GNU_VISIBLE",
        "-I src/sd_driver",
        "-I src/include",
        "-I src/ff15/source"
      ]
    }
  }